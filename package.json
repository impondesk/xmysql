  "repository": "o1lab/xmysql",
  "homepage": "https://github.com/nocodb/nocodb",
  "author": "o1lab",
  "license": "MIT",
  "dependencies": {
    "assert": "^1.4.1",
    "body-parser": "^1.18.2",
    "cluster": "^0.7.7",
    "colors": "^1.1.2",
    "commander": "^2.11.0",
    "cors": "^2.8.4",
    "express": "^4.16.1",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "mocha": "^5.2.0",
